#: class-plugin.php:71
msgid "Min Max Quantity"
msgstr "הגבלת כמות רכישה"

#: class-plugin.php:91
msgid "Minimum Quantity"
msgstr "כמות מינימלית"

#: class-plugin.php:93
msgid "Minimum quantity to purchase, set 1 for no minimum limitation."
msgstr "כמות מינימלית לרכישה, הגדר 1 לביטול הגבלה מינימלית."

#: class-plugin.php:103
msgid "Maximum Quantity"
msgstr "כמות מרבית"

#: class-plugin.php:105
msgid "Maximum quantity to purchase, set 0 for no maximum limitation."
msgstr "כמות מרבית לרכישה, הגדר 0 לביטול הגבלה מקסימלית."

#: class-plugin.php:203 class-plugin.php:269
#, php-format
msgid "You need a quantity of at least %s to add the product to cart."
msgstr "אתה צריך כמות של לפחות %s כדי להוסיף את המוצר לסל."

#: class-plugin.php:209 class-plugin.php:275
#, php-format
msgid "You can purchase a maximum of %s units of this product."
msgstr "באפשרותך לרכוש מקסימום %s יחידות של מוצר זה."

#. translators: 1: WooCommerce 2: Plugin Name
#: wc-min-max-quantity.php:69
#, php-format
msgid ""
"\"%1$s\" is required to be installed and activated in order to use \"%2$s\"."
msgstr "‫“%1$s” נדרש להיום מותקן ומופעל על מנת להשתמש בתוסף “%2$s”."

#: wc-min-max-quantity.php:70
msgid "WooCommerce"
msgstr "ווקומרס"

